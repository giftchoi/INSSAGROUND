<?xml version="1.0" encoding="UTF-8"?>
<!-- Sql Mapper -->
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kosta.inssaground.model.mapper.HobbyMapper">
	<select id="findHobbyByHobbyCategoryNo" resultType="hobbyVO">
		select * from hobby where hobby_Category_No=#{value}
	</select>
	
	<select id="getHobbyCategory" resultType="hobbyCategoryVO">
		select * from hobby_category
	</select>
	
	<select id="findHobbyByHobbyNo" resultType="hobbyVO">
		select * from hobby where hobby_no=#{value}
	</select>
	<select id="getHobbyFeatureAndHobbyFeatureNoList" resultType="FeatureVO">
		select f.feature_no,f.name
		from feature f
	</select>
</mapper>
 